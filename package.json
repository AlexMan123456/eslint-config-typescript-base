{
  "name": "@alextheman/eslint-config-typescript-base",
  "version": "1.0.12",
  "description": "",
  "main": "eslint.config.cjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '*.{cjs,json}' && ESLINT_MODE=fix eslint --fix '*.{cjs,json}'",
    "lint": "ESLINT_MODE=lint eslint '*.{cjs,json}' && prettier --check '*.{cjs,json}'",
    "prepare": "husky",
    "update-dependencies": "npx npm-check-updates -u && npm install",
    "change-major": "npm version major -m \"Change version number to v%s\"",
    "change-minor": "npm version minor -m \"Change version number to v%s\"",
    "change-patch": "npm version patch -m \"Change version number to v%s\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "peerDependencies": {
    "@eslint/js": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "@alextheman/eslint-plugin": "^1.0.16",
    "typescript-eslint": "^8.37.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7"
  }
}
